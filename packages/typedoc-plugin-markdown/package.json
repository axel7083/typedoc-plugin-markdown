{
  "name": "typedoc-plugin-markdown",
  "version": "4.0.0-next.43",
  "description": "A plugin for TypeDoc that enables TypeScript API documentation to be generated in Markdown.",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "prepublishOnly": "npm run lint && npm run build",
    "prebuild": "rm -rf dist && prebuild-options && ts-node scripts/prebuild",
    "build": "tsc",
    "pretest": "ts-node ./test/__scripts__/prepare",
    "test-md": "node test/__scripts__/lint.md.mjs",
    "test-mdx": "node test/__scripts__/lint.mdx.mjs",
    "test": "npm run test-md && npm run test-mdx && jest",
    "test:update": "npm run build && npm test -- -u",
    "build-and-run": "npm run build && npm run pretest",
    "build-and-test": "npm run build && npm run test",
    "docs": "npm run build && typedoc --options ./typedoc.md.json --out ./docs/api && typedoc --options ./typedoc.base.json  --out ./docs/api-html"
  },
  "author": "Thomas Grey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgreyuk/typedoc-plugin-markdown/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/typedoc-plugin-markdown.git",
    "directory": "packages/typedoc-plugin-markdown"
  },
  "homepage": "https://github.com/tgreyuk/typedoc-plugin-markdown/tree/master/packages/typedoc-plugin-markdown",
  "keywords": [
    "markdown",
    "typescript",
    "documentation",
    "api",
    "typedoc-plugin",
    "typedoc"
  ],
  "peerDependencies": {
    "typedoc": "0.25.x"
  }
}
